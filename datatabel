$.ajax(
    {url: "/data",
  success: function(result){
     console.log(result);
     $('#table').DataTable( {
        "lengthMenu": [[10, 20, 50, -1], [10, 20, 50, "All"]],
            "ajax": "/data",
             dataSrc: '',
             result : result,
            columns: [
            {   result:0},
            {  result:1},
            {  result:2},
            {  result:3},
            { result:4},
            {  result:5},


             ]
        } );

  }});
